{
  "agent": {
    "run_as_user": "root"
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/var/log/syslog",
            "log_group_name": "iot-syslog",
            "log_stream_name": "{local_hostname}",
            "retention_in_days": 7
          }
        ]
      }
    }
  }
}
